{
  "cli": {
    "version": ">= 7.1.3"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "env": {
        "ENVIRONMENT": "development"
      },
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "staging": {
      "env": {
        "ENVIRONMENT": "staging"
      },
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "env": {
        "ENVIRONMENT": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
